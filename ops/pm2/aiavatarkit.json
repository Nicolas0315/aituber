{
  "apps": [
    {
      "name": "aiavatarkit-backend",
      "cwd": "./aiavatarkit/examples/aituber",
      "script": "python",
      "args": "-m uvicorn server:app --host 0.0.0.0 --port 8000",
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "env_file": "./aiavatarkit/examples/aituber/.env",
      "autorestart": true,
      "max_restarts": 10,
      "restart_delay": 5000
    }
  ]
}
